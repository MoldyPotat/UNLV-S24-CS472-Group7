#include <iostream>
using namespace std;

// Function to fill an array with multiples of 5
void fillArrayWithMultiplesOfFive(int array[], int size) {
    for (int index = 0; index < size; ++index) {
        array[index] = index * 5; // Calculate and assign multiples of 5
    }
}

int main() {
    const int arraySize = 10; // Size of the array
    int FivesArray[arraySize]; // Array to store multiples of 5

    // Fill the array with multiples of 5
    fillArrayWithMultiplesOfFive(FivesArray, arraySize);

    // Print the array contents
    for (int index = 0; index < arraySize; ++index) {
        cout << FivesArray[index] << " "; // Output each element
    }
    cout << endl;

    return 0;
}
